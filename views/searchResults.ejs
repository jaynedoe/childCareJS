<%- include('partials/header') %>

 <!-- Title Inner-->

 <div class="row">
    <div class="col-lg-8">
        <h1 class="big-heading" style="text-align: left;">Search Results</h1>
        <p style="text-align: left; color: #e43f5a;">Your filtered list of childcare centres.</p>
        <a href="search"><input style="display:block" type="button"
                class="download-btn btn btn-dark btn-lg mb-5 mt-5" value="Search Again"></a> 
    </div>
    <div class="col-lg-4">
    </div>
</div>
</div>
</section>


<!-- Features -->

<section class="white-section">

<div class="container-fluid">
    <div class="row">
        <h2 class="middle-heading mb-5">We found <%= cCCentres.length %> matching centres.</h2>
        <table class="table table-sm table-hover">
            <thead>
                <tr>
                    <th class="left">Centre Name</th>
                    <th>Centre Size</th>
                    <th>Suburb</th>
                    <th>Rating</th>
                    <th>Long Day Care</th>
                    <th>After School Care</th>
                    <th>Before School Care</th>
                    <th>Vacation Care</th>
                    <th>Temporarily Closed</th>
                </tr>
            </thead>
            <tbody>
                <% cCCentres.forEach(function(item){ %>

                    <tr>
                        <td class="left"><%= item.centreName %></td>
                        <td><%= item.centreSize %></td>
                        <td><%= item.suburb %></td>
                        <td><%= item.rating %></td>
                        <td><%= item.longDayCare %></td>
                        <td><%= item.afterSchoolCare %></td>
                        <td><%= item.beforeSchoolCare %></td>
                        <td><%= item.vacationCare %></td>
                        <td><%= item.temporarilyClosed %></td>
                    </tr>

                <% }); %>
            </tbody>
        </table>
    </div>
</div>

</section>


<%- include('partials/footer') %>
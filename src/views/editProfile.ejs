                    <span class="td"><%= household %></span>
                    <span class="td"><select class="form-control" name="editHousehold" id="editHousehold"><option value="Couple">Couple</option><option value="Single">Single</option></select></span>
                    <span class="td"><button class="btn btn-sm btn-dark" type="submit">Submit</button></span>
   

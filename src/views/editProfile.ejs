<%- include('partials/header') %>

<main>

    <div class="container-fluid">

        <div class="row mb-5">
            <h1 class="display-4 mb-3">Edit Profile</h1>
            <hr>
            <a href="/profile"><button class="btn btn-primary">Back to Profile</button></a>
            
        </div>
      
